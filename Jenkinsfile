pipeline {
    agent any 
    
    stages {
        stage('Build') {
            steps {
                sh 'echo "main branch..."'
            }
        }
        
        stage('Test') {
            steps {
                sh 'echo "Testing.. . main"'
            }
        }
        
        stage('Deploy') {
            steps {
                sh 'echo "main Deploying ..f"'
            }
        }
    }
}
